      "id": "run-lint",
      "name": "ğŸ” Execute - Run Linter",
      "type": "n8n-nodes-base.executeCommand",
      "typeVersion": 1,
      "position": [3250, 50]
    },
    {
      "parameters": {
        "jsCode": "const output = $input.item.json.stdout || '';\nconst errorOutput = $input.item.json.stderr || '';\nconst exitCode = $input.item.json.exitCode || 0;\n\n// Parse lint results\nconst lintPassed = exitCode === 0;\nconst errors = (output.match(/(\\d+)\\s+error/) || [null, '0'])[1];\nconst warnings = (output.match(/(\\d+)\\s+warning/) || [null, '0'])[1];\n\nreturn {\n  lint_passed: lintPassed,\n  lint_errors: parseInt(errors) || 0,\n  lint_warnings: parseInt(warnings) || 0,\n  lint_output: output.substring(0, 1000)\n};"
      },
      "id": "parse-lint-results",
      "name": "ğŸ“Š Parse - Lint Results",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [3450, 50]
    },
    {
      "parameters": {
        "command": "cd \"={{$json.body.codebase_path}}\" && npx tsc --noEmit 2>&1",
